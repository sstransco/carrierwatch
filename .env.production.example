# Production environment variables
# Copy to .env and fill in real values

# PostgreSQL / PostGIS
POSTGRES_USER=carrierwatch
POSTGRES_PASSWORD=CHANGE_ME_STRONG_RANDOM_PASSWORD
POSTGRES_DB=carrierwatch
POSTGRES_HOST=postgres
POSTGRES_PORT=5432
DATABASE_URL=postgresql://carrierwatch:CHANGE_ME_STRONG_RANDOM_PASSWORD@postgres:5432/carrierwatch

# Martin tile server
MARTIN_DATABASE_URL=postgresql://carrierwatch:CHANGE_ME_STRONG_RANDOM_PASSWORD@postgres:5432/carrierwatch

# Backend
BACKEND_HOST=0.0.0.0
BACKEND_PORT=8000
CORS_ORIGINS=https://carrier.watch,https://www.carrier.watch

# Frontend (baked into build)
VITE_API_URL=https://carrier.watch
VITE_TILES_URL=https://carrier.watch/tiles
VITE_MAPBOX_TOKEN=your_mapbox_public_token_here

# Domain
DOMAIN=carrier.watch
